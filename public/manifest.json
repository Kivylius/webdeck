{
  "short_name": "Webdeck",
  "name": "Webdeck Studio",
  "manifest_version": 2,
  "version": "1.2.1",
  "description": "Configure you deck using only the web browser, install plugins, and explore custom configurations and presets and profiles.",
  "icons": [
    {
      "src": "favicon.ico",
      "sizes": "48x48",
      "type": "image/x-icon"
    },
    {
      "src": "maskable_icon_x72.png",
      "type": "image/png",
      "sizes": "72x72"
    },
    {
      "src": "maskable_icon_x96.png",
      "type": "image/png",
      "sizes": "96x96"
    },
    {
      "src": "maskable_icon_x128.png",
      "type": "image/png",
      "sizes": "128x128"
    },
    {
      "src": "maskable_icon_x192.png",
      "type": "image/png",
      "sizes": "192x192"
    },
    {
      "src": "maskable_icon_x384.png",
      "type": "image/png",
      "sizes": "384x384"
    },
    {
      "src": "maskable_icon_x512.png",
      "type": "image/png",
      "sizes": "512x512"
    }
  ],
  "screenshots": [    
    {
      "src": "screenshot.png",
      "sizes": "905x456",
      "type": "image/png",
      "form_factor": "wide",
      "label": "Application"
    }
  ],
  "browser_actions": {
    "default_popup": "index.html"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "start_url": ".",
  "display": "standalone",
  "theme_color": "#000000",
  "background_color": "#282828",
  "optional_host_permissions":[
    "https://*/*",
    "http://*/*"
  ]
}
